{
  "name": "kokifi-lottery",
  "version": "1.0.0",
  "description": "KoquiFI Lottery - Farcaster Frame miniapp with Base Network integration",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "contracts:compile": "forge build",
    "contracts:test": "forge test",
    "contracts:deploy": "forge script script/Deploy.s.sol --rpc-url base-sepolia --broadcast --verify",
    "contracts:deploy-local": "forge script script/Deploy.s.sol --rpc-url http://localhost:8545 --broadcast",
    "dev:full": "concurrently \"npm run dev\" \"npm run contracts:deploy-local\""
  },
  "dependencies": {
    "@farcaster/miniapp-sdk": "^0.1.10",
    "@neynar/nodejs-sdk": "^2.0.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.15.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/sqlite3": "^3.1.11",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.294.0",
    "next": "^14.0.0",
    "pg": "^8.16.3",
    "postcss": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "viem": "^2.0.0",
    "wagmi": "^2.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0"
  },
  "keywords": [
    "farcaster",
    "lottery",
    "base",
    "blockchain",
    "miniapp",
    "frames"
  ],
